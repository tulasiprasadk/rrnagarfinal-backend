{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "^/upload-demo.html$", "dest": "/public/upload-demo.html" },
    { "src": "^/$", "dest": "/api/index.js" },
    { "src": "^(?!/api/)(.*)$", "dest": "/api/index.js" }
  ]
}
